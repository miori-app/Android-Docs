package kr.ac.sejong.appprogramming.java1;

public class App {

	public static void main(String[] args) {
		Circle c1 = new Circle();
		c1.x = 3;
		c1.y = 4;
		c1.r = 5;
		System.out.println(c1.getArea());
		
		Circle c2 = new Circle();
		c2.x=2;
		c2.y=3;
		c2.r=3;
		System.out.println(c2.getArea());
	}
}
